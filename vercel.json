{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram-webhook",
      "dest": "/api/telegram-webhook.ts"
    },
    {
      "src": "/",
      "methods": ["POST"],
      "dest": "/api/telegram-webhook.ts"
    }
  ]
}

